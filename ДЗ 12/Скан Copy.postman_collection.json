		{
			"name": "Скан Интерфакс",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const testType = pm.iterationData.get(\"test_type\");\r",
							"\r",
							"if (testType === \"positive\") {\r",
							"    pm.test(\"Status 200\", () => pm.response.to.have.status(200));\r",
							"    // Установка токена\r",
							"} \r",
							"else {\r",
							"    pm.test(\"Status 400 or 401\", () => pm.expect(pm.response.code).to.be.oneOf([400, 401]));\r",
							"}\r",
							"\r",
							"const response = pm.response.json();\r",
							"if (response && response.token) {\r",
							"    pm.collectionVariables.set(\"My_key\", response.token);\r",
							"}\r",
							"\r",
							"pm.test(\"Негативный тест: неверный логин или пароль\", () => {\r",
							"    pm.response.to.have.status(200);\r",
							"    const response = pm.response.json();\r",
							"    pm.expect(response.success).to.be.false;\r",
							"});\r",
							"\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"auth": {
					"type": "bearer",
					"bearer": [
						{
							"key": "token",
